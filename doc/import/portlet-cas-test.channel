<channel-definition script="classpath://org/jasig/portal/io/import-channel_v3-0.crn">
  <title>Portlet CAS Test</title>
  <name>Portlet CAS Test</name>
  <fname>portlet-cas-test</fname>
  <desc>Tests uPortal's ability to pass a valid CAS ticket to a portlet through the UserInfo map.</desc>
  <type>Portlet</type>
  <class>org.jasig.portal.channels.portlet.CSpringPortletAdaptor</class>
  <timeout>60000</timeout>
  <hasedit>Y</hasedit>
  <hashelp>N</hashelp>
  <hasabout>N</hasabout>
  <secure>N</secure>
  <locale>en_US</locale>
  <categories>
    <category>Development</category>
  </categories>
  <groups>
    <group>Everyone</group>
  </groups>
  <parameters>
    <parameter>
      <name>portletApplicationId</name>
      <value>/cas-proxy-test-portlet</value> 
      <description/>  
      <ovrd>N</ovrd>
    </parameter>
    <parameter>
      <name>portletName</name>
      <value>ProxyCasTest</value> 
      <description/>  
      <ovrd>N</ovrd>
    </parameter>
  </parameters>
</channel-definition>